      </pss:aclfLine>
      <pss:aclfXfr areaNumber="1" zoneNumber="1" circuitId="1" id="Branch_2">
      	<pss:fromBus idRef="Bus1"></pss:fromBus>
      	<pss:toBus idRef="Bus2"></pss:toBus>
      </pss:aclfXfr>
      <pss:aclfPSXfr areaNumber="1" zoneNumber="1" circuitId="1" id="Branch_3">
      	<pss:fromBus idRef="Bus1"></pss:fromBus>
      	<pss:toBus idRef="Bus2"></pss:toBus>
      </pss:aclfPSXfr>
    </pss:branchList>
  </pss:aclfNet>
</pss:pssStudyCase>