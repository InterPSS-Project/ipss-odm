<jaxb:bindings version="2.1"
    xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc">
    <jaxb:bindings schemaLocation="ODMschema.xsd" node="/xsd:schema">
        <jaxb:schemaBindings>
            <jaxb:package name="org.interpss.schema"/>
        </jaxb:schemaBindings>
         <jaxb:globalBindings>
            <xjc:simple>true</xjc:simple>
            <xjc:generateDateStamp>false</xjc:generateDateStamp>
        </jaxb:globalBindings>
    </jaxb:bindings>
</jaxb:bindings>